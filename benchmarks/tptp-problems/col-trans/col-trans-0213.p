include('col-axioms.ax').

fof(pipo,conjecture,
(! [A, B, C , D, T, X] : ( (
 wd( A, D) &
 wd( B, D) &
  wd( C, D) &
   wd( D, T) &
    wd( B, C) &
     wd( C, T) &
      wd( B, T) &
       wd( A, B) &
        wd( A, C) &
         wd( A, T) &
          wd( X, A) &
           col( A, B, X) &
            col( X, T, A) &
             col( B, D, C) & col( A, D, T) ) => col( B, C, T))) 
).

