include('col-axioms.ax').

fof(pipo,conjecture,
(! [O, E, Eprime, A] : ( (
 wd( O, Eprime) &
 wd( O, A) &
  wd( O, E) &
   wd( O, Eprime) &
    wd( E, Eprime) &
     col( O, E, A) &
      col( O, E, O) &
       col( O, E, A) &
        col( O, Eprime, A) &
         col( O, O, A) & col( Eprime, O, A) ) => col( O, E, Eprime)))  ).

