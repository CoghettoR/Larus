include('col-axioms.ax').

fof(pipo,conjecture,
(! [O, E, Eprime, A, B, C] : ( (
 wd( A, O) &
 wd( O, E) &
  wd( E, Eprime) &
   wd( O, Eprime) &
    wd( O, B) &
     wd( B, C) &
      wd( C, A) &
       wd( O, C) &
        wd( B, A) &
         col( O, E, A) &
          col( O, E, B) & col( O, E, C) ) => col( A, O, C)))  ).

