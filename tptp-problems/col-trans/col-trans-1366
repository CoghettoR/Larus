fof(pipo,conjecture,
(! [O, E, Eprime, B, C, U] : ( (
 wd( U, O) &
 wd( O, E) &
  wd( O, Eprime) &
   wd( E, Eprime) &
    wd( O, O) &
     wd( B, O) &
      wd( C, O) &
       col( O, E, O) &
        col( O, E, B) &
         col( O, E, C) &
          col( O, E, O) &
           col( O, E, U) & col( O, U, Eprime) ) => col( O, E, Eprime)))  ).

